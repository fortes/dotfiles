# Avoid copying VCS metadata and local caches into the image
.git
.gitmodules
.github

# Throwaway local output
node_modules
**/__pycache__
**/*.pyc
**/*.pyo
**/*.o
**/*.so
*.log
*.swp
*.swo
.DS_Store

# Tool-specific caches that can blow up context size
.cache
.venv
uv-project

# Editor/OS cruft
.idea
.vscode

# Mirror sensitive/generated files ignored in repo
stowed-files/ssh/.ssh/*.local
stowed-files/ssh/.ssh/*.pub
stowed-files/ssh/.ssh/*_rsa
stowed-files/ssh/.ssh/known_hosts
stowed-files/epy/.config/epy/states.db
stowed-files/nvim/.config/nvim/.netrwhist
stowed-files/nvim/.config/nvim/lazy-lock.json
stowed-files/nvim/.config/nvim/plugin/
stowed-files/nvim/.config/nvim/spell/*.spl
